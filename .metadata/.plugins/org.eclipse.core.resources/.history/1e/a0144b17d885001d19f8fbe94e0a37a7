
public class DataProcessor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
				String json="""{
								"id":1,
								"name":"tom",
								"age":20}""";
				int len=json.length();
				int size=0;
				for(int i=0;i<len;i++) {
					size++;
					if(size==250) {
						continue;
					}
					System.out.println(json.charAt(i));
	}

}
