//arr=[13,15,17,17,21,27]
//k=36



public class example {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[]= {13,15,17,17,21,27};
		int k=36;
		int l=0;
		int r=arr.length-1;
		while(l<r) {
			if(arr[l]+arr[r]==k)
				System.out.println(arr[l]+","+arr[r]);
			else if (arr[l]+arr[r]<k)
				l++;
			else
				r--;
		}
	}

}
